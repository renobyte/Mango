<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="70dp"
	android:minHeight="70dp"
	android:id="@+id/favoritesRowLayout"
	android:gravity="right">
	<ImageView
		android:id="@+id/favoritesRowThumb"
		android:layout_height="70dp"
		android:paddingTop="2dp"
		android:paddingLeft="2dp"
		android:layout_width="70dp"
		android:src="@drawable/coverart_overlay"
		android:scaleType="fitXY"
		android:layout_centerVertical="true" />
	<TextView
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		android:layout_toRightOf="@+id/favoritesRowThumb"
		android:layout_toLeftOf="@+id/favoritesRowIconLayout"
		android:id="@+id/favoritesRowTitle"
		android:layout_alignParentTop="true"
		android:textSize="15sp"
		android:textStyle="bold"
		android:text="Favorite Title"
		android:paddingLeft="1dp"
		android:singleLine="true"
		android:ellipsize="marquee"
		android:marqueeRepeatLimit="marquee_forever"></TextView>
	<TextView
		android:layout_height="wrap_content"
		android:maxLines="1"
		android:layout_width="wrap_content"
		android:textSize="14sp"
		android:layout_marginTop="-4dp"
		android:paddingLeft="1dp"
		android:text="My Progress: v01/c001 page 1"
		android:layout_below="@+id/favoritesRowTitle"
		android:layout_alignLeft="@+id/favoritesRowTitle"
		android:layout_toLeftOf="@+id/favoritesRowIconLayout"
		android:textColor="#555555"
		android:id="@+id/favoritesRowProgress"></TextView>
	<TextView
		android:layout_height="wrap_content"
		android:maxLines="1"
		android:layout_width="wrap_content"
		android:textSize="14sp"
		android:paddingLeft="1dp"
		android:layout_marginTop="-2dp"
		android:text="Updated June 1 (v02/c004)"
		android:id="@+id/favoritesRowUpdate"
		android:layout_below="@+id/favoritesRowProgress"
		android:layout_alignLeft="@+id/favoritesRowTitle"
		android:layout_toLeftOf="@+id/favoritesRowIconLayout"
		android:textColor="#555555"></TextView>
	<TextView
		android:layout_height="wrap_content"
		android:maxLines="1"
		android:layout_width="wrap_content"
		android:textSize="12sp"
		android:paddingLeft="2dp"
		android:text="MangaFox"
		android:id="@+id/favoritesRowSite"
		android:layout_below="@+id/favoritesRowUpdate"
		android:layout_alignLeft="@+id/favoritesRowTitle"
		android:layout_toLeftOf="@+id/favoritesRowIconLayout"
		android:textColor="#555555"></TextView>
	<LinearLayout
		android:layout_alignParentRight="true"
		android:id="@+id/favoritesRowIconLayout"
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		android:layout_gravity="bottom"
		android:orientation="vertical">
		<ImageView
			android:id="@+id/favoritesRowIconTag"
			android:src="@drawable/tag_blue"
			android:layout_width="32dp"
			android:layout_height="32dp"
			android:scaleType="fitXY" />
	</LinearLayout>
	<LinearLayout
		android:paddingRight="2dp"
		android:id="@+id/linearLayout1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:paddingBottom="2dp"
		android:layout_alignParentRight="true"
		android:layout_alignParentBottom="true">
		<ImageView
			android:layout_width="wrap_content"
			android:id="@+id/favoritesRowIcon2"
			android:src="@drawable/icon_notifications"
			android:scaleType="center"
			android:paddingRight="2dp"
			android:layout_height="wrap_content"></ImageView>
		<ImageView
			android:layout_width="wrap_content"
			android:id="@+id/favoritesRowIcon1"
			android:src="@drawable/icon_libraryseries"
			android:scaleType="center"
			android:layout_height="wrap_content"></ImageView>
	</LinearLayout>
</RelativeLayout>