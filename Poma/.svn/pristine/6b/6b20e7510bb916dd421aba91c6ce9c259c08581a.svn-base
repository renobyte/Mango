<?xml version="1.0" encoding="utf-8"?>
<com.ls.manga.ui.PomaLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<com.ls.manga.ui.PomaAdWrapperView
		android:id="@+id/siteselectAdLayout"
		android:layout_alignParentTop="true"
		android:layout_height="50dip"
		android:layout_width="fill_parent"
		android:gravity="center_vertical|center_horizontal">
	</com.ls.manga.ui.PomaAdWrapperView>
	<LinearLayout
		android:id="@+id/siteselectBottomLayout"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		android:layout_alignParentBottom="true">

	</LinearLayout>
	<LinearLayout
		android:id="@+id/siteselectListLayout"
		android:layout_below="@id/siteselectAdLayout"
		android:layout_above="@id/siteselectBottomLayout"
		android:layout_height="fill_parent"
		android:layout_width="fill_parent">
		<ListView
			android:id="@+id/siteselectList"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:drawSelectorOnTop="false"
			android:background="@android:color/transparent"
			android:cacheColorHint="#00000000"
			android:fadingEdge="none"></ListView>
	</LinearLayout>
	<include
		layout="@layout/tutorial_overlay"
		android:layout_height="fill_parent"
		android:layout_width="fill_parent">
	</include>
</com.ls.manga.ui.PomaLayout>
