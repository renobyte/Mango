<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:minHeight="40dp"
	android:id="@+id/ilr2RowHolder">
	<View
		android:id="@+id/ilr2ReadTag"
		android:layout_width="5dp"
		android:layout_height="fill_parent"
		android:paddingRight="2dp"
		android:background="#555555"></View>
	<RelativeLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<ImageView
			android:id="@+id/ilr2Icon"
			android:src="@drawable/ic_error"
			android:layout_height="wrap_content"
			android:layout_toRightOf="@+id/ilr2ReadTag"
			android:paddingTop="2dp"
			android:paddingLeft="2dp"
			android:layout_width="wrap_content"
			android:layout_centerVertical="true" />
		<TextView
			android:id="@+id/ilr2Label"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@+id/ilr2Icon"
			android:maxLines="2"
			android:textSize="18sp"
			android:layout_width="fill_parent"
			android:paddingRight="3dp"
			android:paddingLeft="3dp"
			android:text="History's Strongest Disciple Kenichi: Chapter 251"/>
		<TextView
			android:layout_height="wrap_content"
			android:maxLines="1"
			android:layout_width="wrap_content"
			android:textSize="12sp"
			android:paddingLeft="3dp"
			android:text="MangaFox"
			android:id="@+id/ilr2Site"
			android:layout_below="@+id/ilr2Label"
			android:layout_alignLeft="@+id/ilr2Label"
			android:textColor="#666666"></TextView>
	</RelativeLayout>
</LinearLayout>